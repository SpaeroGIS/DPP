<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="milspace" type="System.Configuration.ConfigurationSectionGroup, System.Configuration, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
      <section name="loadActions" type="MilSpace.Configurations.LoadActionAssembliesSection, MilSpace.Configurations, Version=1.0.0.0, Culture=neutral, PublicKeyToken=9d4c36abea5650da" />
      <sectionGroup name="connections" type="MilSpace.Configurations.ConnectionsSection, MilSpace.Configurations, Version=1.0.0.0, Culture=neutral, PublicKeyToken=9d4c36abea5650da" >
        <section name="workingDB" type="MilSpace.Configurations.WorkingDBConnectionSection, MilSpace.Configurations, Version=1.0.0.0, Culture=neutral, PublicKeyToken=9d4c36abea5650da" />
        <section name="workingGDB" type="MilSpace.Configurations.WorkingGDBConnectionSection, MilSpace.Configurations, Version=1.0.0.0, Culture=neutral, PublicKeyToken=9d4c36abea5650da" />
        <section name="temporatyGDB" type="MilSpace.Configurations.TemporaryGDBConnectionSection, MilSpace.Configurations, Version=1.0.0.0, Culture=neutral, PublicKeyToken=9d4c36abea5650da" />
      </sectionGroup>
      <section name="python" type="MilSpace.Configurations.Python.PythonConfiguratuinSection, MilSpace.Configurations"/>
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <milspace>
    <loadActions>
      <actions>
        <add name="milspacecore" assembly="MilSpace.Core" />
        <add name="milspacetools" assembly="MilSpace.Tools" />
      </actions>
    </loadActions>
    <connections>
      <workingDB connectionString="Data Source=SRVDEV\SQLEXPR2008;Integrated Security=True;Initial Catalog=SB_WORK_TS4"  />
      <workingGDB connectionString="Data Source=SRVDEV\SQLEXPR2008;Integrated Security=True;Initial Catalog=SB_WORK_TS4" />
      <temporatyGDB connectionString="E:\Data\MilSpace3D\MilSpaceCalc_40a.gdb"  />
    </connections>
    
    <python scriptsStorage ="d:\Projects\AgroGIS\Python\" runWith="c:\Python27\ArcGIS10.1\python.exe" />
  </milspace>
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="MilSpace.log"/>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="4"/>
      <maximumFileSize value="1MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d [%t] %-5p %c %m%n"/>
      </layout>
    </appender>

    <root>
      <level value="ALL" />
      <appender-ref ref="LogFileAppender"/>
    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.ValueTuple" publicKeyToken="cc7b13ffcd2ddd51" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.3.0" newVersion="4.0.3.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>